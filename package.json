{
  "name": "obsidian-sample-plugin",
  "version": "0.11.13",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env BUILD=dev obsidian-plugin dev src/main.ts",
    "build": "cross-env BUILD=production obsidian-plugin build src/main.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "sort-imports": "import-sort --write \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clean": "yarn sort-imports && yarn prettier && yarn lint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/diff": "^5.0.0",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "cross-env": "^7",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module-alias": "^1.1.0",
    "obsidian": "^0.12.11",
    "obsidian-dataview": "^0.4.5",
    "obsidian-plugin-cli": "^0.8.0",
    "prettier": "^2.3.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@textcomplete/core": "^0.1.9",
    "@textcomplete/textarea": "^0.1.9",
    "@types/box-intersect": "^1.0.0",
    "@types/insert-text-at-cursor": "^0.3.0",
    "@types/js-yaml": "^4.0.1",
    "@types/mark.js": "^8.11.5",
    "@types/raf-schd": "^4.0.1",
    "@types/react-dom": "^17.0.3",
    "animated-scroll-to": "^2.2.0",
    "box-intersect": "^1.0.2",
    "choices.js": "^9.0.1",
    "classcat": "^5.0.3",
    "deepmerge": "^4.2.2",
    "diff": "^5.0.0",
    "flatpickr": "^4.6.9",
    "fuse.js": "^6.4.6",
    "immutability-helper": "^3.1.1",
    "insert-text-at-cursor": "^0.3.0",
    "is-plain-object": "^5.0.0",
    "js-yaml": "^4.1.0",
    "mark.js": "^8.11.1",
    "monkey-around": "^2.1.0",
    "preact": "^10",
    "raf-schd": "^4.0.3",
    "react": "npm:@preact/compat",
    "react-cool-onclickoutside": "^1.6.1",
    "react-dom": "npm:@preact/compat"
  },
  "importSort": {
    ".js, .ts, .tsx": {
      "style": "module-alias",
      "options": {
        "alias": [
          "src"
        ]
      }
    }
  }
}
